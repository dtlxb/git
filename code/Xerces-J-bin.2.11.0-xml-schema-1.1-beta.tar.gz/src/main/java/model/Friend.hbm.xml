<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Friend" table="friend">
		<id name="id" type="int">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="uid1" type="int">
			<column name="uid1" />
		</property>
		<property name="uid2" type="int">
			<column name="uid2" />
		</property>
		<property name="state" type="int">
			<column name="state" />
		</property>
	</class>
</hibernate-mapping>