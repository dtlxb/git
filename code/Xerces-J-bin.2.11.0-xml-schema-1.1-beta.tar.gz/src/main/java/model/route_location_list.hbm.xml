<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.route_location_list" table="route_location_list">
 
		<composite-id >  
		  <key-property name="id" type="int" column="rid"/>  
           <key-property  name="index" type="int" column="ind"/>
 
        </composite-id>
 
 
		<property name="latitude" type="double">
			<column name="latitude" />
		</property>
		<property name="longitude" type="double">
			<column name="longitude" />
		</property>

	</class>
</hibernate-mapping>