                    android:background="@color/line" />

            </RelativeLayout>

            <FrameLayout
                android:id="@+id/layout_industry"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </FrameLayout>

            <View
                android:layout_width="match_parent"
